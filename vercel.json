{
    "version": 2,
    "builds": [
        {
            "src": "api/index.ts",
            "use": "@vercel/node"
        },
        {
            "src": "react-frontend/package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/index.ts"
        },
        {
            "src": "/halo/(.*)",
            "dest": "api/index.ts"
        },
        {
            "src": "/merchant/(.*)",
            "dest": "api/index.ts"
        },
        {
            "src": "/(.*)",
            "dest": "react-frontend/$1"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}